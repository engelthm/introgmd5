<!-- lottie --> <script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
<!-- bodymovin --> <script src="https://cdnjs.cloudflare.com/ajax/libs/bodymovin/5.9.6/lottie.min.js"></script>
<!-- tippy.js --> <script src="https://unpkg.com/popper.js@1"></script><script src="https://unpkg.com/tippy.js@5/dist/tippy-bundle.iife.js"></script>
<!-- tilt.js --> <script src="https://cdnjs.cloudflare.com/ajax/libs/vanilla-tilt/1.7.0/vanilla-tilt.min.js"></script>
<!-- lightbox --> <script src="magnific-popup/jquery.magnific-popup.js"></script>

<script> 
// lottie/bodymovin icon animation
let iconMenu = document.querySelector('.clickmenu');
let animationMenu = bodymovin.loadAnimation({
    container: iconMenu,
    renderer: 'svg',
    loop: false,
    autoplay: false,
    path: "https://raw.githubusercontent.com/engelthm/introgmd5/main/assets/menuV2"
    });
    var directionMenu = 1;
        iconMenu.addEventListener('click', (e) => {
        animationMenu.setDirection(directionMenu);
        animationMenu.play();
        directionMenu = -directionMenu;
});

// tippy.js
tippy('[title]', {
    theme: 'custom',
    arrow: tippy.roundArrow,
    followCursor: true,
    zIndex: 9999999999,
    maxWidth: 300,
    content(reference) {
        const title = reference.getAttribute('title');
        reference.removeAttribute('title');
        return title;
    },
});

// dropdown navigation
var navi = document.getElementsByClassName("click");
var i;
for (i = 0; i < navi.length; i++) {
    navi[i].addEventListener("click", function() {
    var naviwrap = this.nextElementSibling;
    if (naviwrap.style.maxHeight){
        naviwrap.style.maxHeight = null;
        } else {
        naviwrap.style.maxHeight = naviwrap.scrollHeight + "px";
        } 
    });
}

// tilt.js
$('.sticker').tilt({
    glare: true,
    maxGlare: 1.0
})

// lightbox
$(document).ready(function() {
    $('.lightbox').magnificPopup({
		type: 'image',
		closeOnContentClick: true,
		closeBtnInside: false,
		fixedContentPos: true,
		mainClass: 'mfp-no-margins mfp-with-zoom',
		image: {
			verticalFit: true
		},
		zoom: {
			enabled: true,
			duration: 300 
		}
	});
});

// removes tumblr redirects
function noHrefLi(){
    var linkSet = document.querySelectorAll('a[href*="href.li/?"]');
    Array.prototype.forEach.call(linkSet,function(el,i){
        var theLink = linkSet[i].getAttribute('href').split("href.li/?")[1];
        linkSet[i].setAttribute("href",theLink);
    });
}
noHrefLi(); 
</script>
